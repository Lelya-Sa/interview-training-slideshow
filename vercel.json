{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm run install-all",
  "rewrites": [
    {
      "source": "/api/roadmap/days/:dayNumber",
      "destination": "/api/roadmap/days?dayNumber=:dayNumber"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "framework": null
}
