{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm run install-all",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/roadmap/days/([^/]+)",
      "dest": "/api/roadmap/days/[dayNumber]?dayNumber=$1"
    }
  ],
  "framework": null
}
