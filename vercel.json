{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm run install-all",
  "functions": {
    "api/**/*.js": {
      "includeFiles": [
        "full_stack_interview_answers.md",
        "daily-schedule/**"
      ]
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "framework": null
}
